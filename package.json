  },
  "devDependencies": {
    "@angular/compiler-cli": "8.0.0",
    "@types/d3": "^5.0.0",
    "@types/globby": "^8.0.0",
    "@types/jasmine": "^3.3.13",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^10.12.18",
    "copyfiles": "^2.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.2.0"
  },
  "scripts": {
    "compile": "run-s compile:*",
    "compile:app": "tsc --noEmit --project tsconfig.app.json",
    "compile:e2e": "tsc --noEmit --project tsconfig.e2e.json",
    "compile:spec": "tsc --noEmit --project tsconfig.spec.json",
    "e2e-all": "run-p -r json e2e-serve protractor",
    "e2e-serve": "live-server --no-browser --no-css-inject --proxy=/api:http://localhost:8085 --watch=DONTWATCHE2E",
    "install": "webdriver-manager update",
    "json": "json-server --port 8085 --host 0.0.0.0 --delay 200 -q --watch demo-data/db.json",
    "lint": "tslint \"**/*.ts?(x)\" --exclude \"**/node_modules/**\" --exclude \"More/**\" --exclude \"*SKIP*/**\" --project tsconfig.json",
    "lint:failbuild": "tslint \"**/*.ts?(x)\" --exclude \"**/node_modules/**\" --exclude \"More/**\" --exclude \"*SKIP*/**\" --project tsconfig.json --config tslint.failbuild.json",
    "lint:fix": "tslint --fix \"**/*.ts?(x)\" --exclude \"**/node_modules/**\" --exclude \"More/**\" --exclude \"*SKIP*/**\" --project tsconfig.json",
    "prettier": "prettier --check \"**/*.{html,ts,css,scss,json}\"",
    "prettier:fix": "prettier --write \"**/*.{html,ts,css,scss,json}\"",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "protractor": "protractor --maxVersions.chrome 73",
    "serve": "node serve.js",
    "start": "run-p -r json serve"
  }
}
